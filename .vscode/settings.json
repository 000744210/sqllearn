{
  "files.exclude": {
    ".babelrc": true,
    "**/.gitignore": true,
    "**/.vscode": true,
    "**/dist": true,
    "**/node_modules": true,
    "package-lock.json": true
  },
  "editor.tabSize": 2,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "prettier.printWidth": 200,
  "search.exclude": {
    "**/dist": true
  }
}
